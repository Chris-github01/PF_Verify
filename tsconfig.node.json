  "compilerOptions": {
    "composite": true,
    "skipLibCheck": true,
    "module": "ESNext",
    "moduleResolution": "bundler",
    "allowSyntheticDefaultImports": true,
    "resolveJsonModule": true,
    "strict": true,
    "noEmit": true,
    "target": "ES2022",
    "lib": ["ES2023"]
  },
  "include": ["vite.config.js"]
}
